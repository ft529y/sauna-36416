<%= render "shared/header_logo" %>

<div class="event-add-wrap">
  <h1 class="event-add-top">予定を入力して下さい</h1>

  <%= form_with model: @event, local: true do |f| %>

  <%= render 'shared/error_messages', model: f.object %>

    <div>
      <%= f.collection_select(:execution_id, Execution.all, :id, :name, {}, class:"event-new-select") %>
    </div>

    <div class="place">
      <%= f.label :place %>
      <%= f.text_field :place %>
    </div>

    <div class="fee">
      <%= f.label :fee %>
      <%= f.text_field :fee %>
    </div>

      <div class="time">
      <%= f.label :start_time %>
      <%= f.datetime_select :start_time %>
    </div>


    <div class="submit">
      <%= f.submit %>
    </div>
</div>

<% end %>